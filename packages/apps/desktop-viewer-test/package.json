    "@types/react-dom": "^17.0.9",
    "cpx2": "4.2.0",
    "cross-env": "^5.2.1",
    "electron-devtools-installer": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "typescript": "~4.4.0",
    "webpack": "4.42.0"
  },
  "eslintConfig": {
    "extends": [
      "../../../common/scripts/.eslintrc.ts.json"
    ]
  }
