{
  "name": "@itwin/desktop-viewer-test",
  "version": "0.1.0",
  "private": true,
  "os": [
    "win32",
    "linux",
    "darwin"
  ],
  "engines": {
    "node": ">=12.17.0 <17.0.0"
  },
  "scripts": {
    "build": "",
    "build:desktop": "npm run -s build:frontend && npm run -s build:backend",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "react-scripts build",
    "clean": "rimraf lib build dist",
    "copy": "npm run copy:assets && npm run copy:enUS",
    "copy:assets": "cpx \"./assets/*\" ./build/assets/",
    "copy:enUS": "cpx \"./build/locales/en/*\" ./build/locales/en-US/",
    "electron": "electron lib/backend/main.js",
    "electron:debug": "cross-env NODE_ENV=development electron lib/backend/main.js",
    "lint": "eslint --config package.json --no-eslintrc \"./src/**/*.{ts,tsx}\" 1>&2",
    "start": "npm run build:backend && run-p \"start:frontend\" \"electron:debug\"",
    "start:frontend": "react-scripts start",
    "test": ""
  },
  "browserslist": [
    "electron 22.0.0"
  ],
  "dependencies": {
    "@bentley/icons-generic-webfont": "^1.0.15",
    "@itwin/appui-abstract": "4.0.0-dev.9",
    "@itwin/appui-layout-react": "4.0.0-dev.9",
    "@itwin/appui-react": "4.0.0-dev.9",
    "@itwin/components-react": "4.0.0-dev.9",
    "@itwin/core-backend": "4.0.0-dev.22",
    "@itwin/core-bentley": "4.0.0-dev.22",
    "@itwin/core-common": "4.0.0-dev.22",
    "@itwin/core-electron": "4.0.0-dev.22",
    "@itwin/core-frontend": "4.0.0-dev.22",
    "@itwin/core-geometry": "4.0.0-dev.22",
    "@itwin/core-i18n": "4.0.0-dev.22",
    "@itwin/core-markup": "4.0.0-dev.22",
    "@itwin/core-orbitgt": "4.0.0-dev.22",
    "@itwin/core-quantity": "4.0.0-dev.22",
    "@itwin/core-react": "4.0.0-dev.9",
    "@itwin/core-telemetry": "4.0.0-dev.22",
    "@itwin/ecschema-metadata": "4.0.0-dev.22",
    "@itwin/electron-authorization": "^0.8.3",
    "@itwin/express-server": "4.0.0-dev.22",
    "@itwin/desktop-viewer-react": "workspace:*",
    "@itwin/imodel-browser-react": "^0.12.2",
    "@itwin/imodel-components-react": "4.0.0-dev.9",
    "@itwin/imodels-access-backend": "^1.0.0",
    "@itwin/imodels-access-frontend": "^1.0.1",
    "@itwin/itwinui-css": "^0.18.1",
    "@itwin/itwinui-icons-react": "^1.2.0",
    "@itwin/itwinui-react": "^1.16.2",
    "@itwin/presentation-backend": "4.0.0-dev.22",
    "@itwin/presentation-common": "4.0.0-dev.22",
    "@itwin/presentation-components": "4.0.0-dev.15",
    "@itwin/presentation-frontend": "4.0.0-dev.22",
    "@itwin/webgl-compatibility": "4.0.0-dev.22",
    "dotenv-flow": "^3.2.0",
    "electron": "^22.0.0",
    "minimist": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
